<Application
    x:Class="FolderIconSetter.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:FolderIconSetter"
    StartupUri="Windows/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="./Themes/CloseButton.xaml" />
                <ResourceDictionary Source="./Themes/RoundedFolderTextBox.xaml" />
                <ResourceDictionary Source="./Themes/RoundedIconTextBox.xaml" />
                <ResourceDictionary Source="./Themes/RoundedButton.xaml" />
                <ResourceDictionary Source="./Themes/GithubButton.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
